#include<bits/stdc++.h>
using namespace std;
int main(){
    vector<int> v = {10,20,30,40,50};
    v.push_back(100);
    cout<< *(v.begin()+2)<<endl;
    v.insert(v.begin()+1,200);
    for(int i:v){
        cout<< i <<" ";
    }
    cout<<endl;
    sort(v.begin(),v.end(),greater<int>());
    for(int i:v){
        cout<< i <<" ";
    }
}
