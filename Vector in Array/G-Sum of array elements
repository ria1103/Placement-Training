#include<bits/stdc++.h>
using namespace std;

int main() {
    int t;
    long int sum,n;
    cin >> t;
    while(t--){
        cin >> n;
        vector<long long int> v(n);;
        sum=0;
        for(int i=0; i<n; i++){
            cin>>v[i];
        }
        for(int i=0; i<n; i++){
            sum+=v[i];
        }
        cout<<sum<<endl;
    }
}
