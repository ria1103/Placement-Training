#include<bits/stdc++.h>
using namespace std;

int main() {
    int n,i,j,k,flag=0;
    cin >> n >> k;
    vector<long int> v(n);
    for(i = 0; i < n; i++) {
        cin >> v[i];
    }
    for(i=0; i<n; i++){
        if(v[i] == k){
            flag=1;
            cout<<i;
        }
    }
    if(flag==0){
        cout<<-1;
    }
    cout<<endl;
    return 0;
}
