#include<bits/stdc++.h>
using namespace std;

int main() {
    int r,c,k;
    cin>>r>>c;
    vector<vector<int>> v(r,vector<int> (c));
    for(int i=0; i<r; i++){
        for(int j=0; j<c; j++){
            cin>>v[i][j];
        }
    }
    for(int i=0; i<r; i++){
        for(int j=0; j<c; j++){
            cin>>k;
            cout<<v[i][j]+k<<" ";
        }
        cout<<"\n";
    }
}
