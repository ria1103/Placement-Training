#include<bits/stdc++.h>
using namespace std;

int main() {
    int n,t,k=0;
    cin>>t;
    while(t--){
        cout<<"Test Case #"<<k+1<<":\n";
        cin>>n;
        vector<vector<int>> v(n,vector<int> (n));
        for(int i=0; i<n; i++){
            for(int j=0; j<n; j++){
                cin>>v[i][j];
            }
        }
        for(int i=0; i<n; i++){
            for(int j=n-1; j>=0; j--){
                cout<<v[j][i]<<" ";
            }
            cout<<"\n";
        }
    }
}
