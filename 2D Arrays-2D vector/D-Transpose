#include<bits/stdc++.h>
using namespace std;

int main() {
    int r,c;
    cin>>r>>c;
    vector<vector<long int>> v(r,vector<long int> (c));
    for(int i=0; i<r; i++){
        for(int j=0; j<c; j++){
            cin>>v[i][j];
        }
    }
    for(int i=0; i<c; i++){
        for(int j=0; j<r; j++){
            cout<<v[j][i]<<" ";
        }
        cout<<"\n";
    }
}
